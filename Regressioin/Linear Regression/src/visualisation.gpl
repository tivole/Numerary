#!/usr/bin/gnuplot

set title "Linear Regression"
set xlabel "x"
set ylabel "y"
set xrange [0.000000:1.000000]

set for [i=1:3] linetype i dt i

set term pngcairo dashed
set output 'visualisation.png'

plot (x*4.557222 + 0.368217) ls 1 lc rgb '#00bb00' lw 1.4 title 'x*4.557222 + 0.368217',\
	(x*4.573584 + 0.362027) ls 2 lc rgb '#0000ff' lw 1.4 title 'x*4.573584 + 0.362027',\
	'input_xy.csv' with points ls 7 lc rgb '#ff0000' ps 1 title 'Input Values'

pause -1 "Hit any key to continue"
